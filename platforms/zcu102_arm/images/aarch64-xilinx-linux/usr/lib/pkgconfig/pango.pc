prefix=/usr
libdir=${prefix}/lib
includedir=${prefix}/include

Name: Pango
Description: Internationalized text handling
Version: 1.44.6
Requires: gobject-2.0 >=  2.59.2, harfbuzz >=  2.0.0
Requires.private: glib-2.0 >=  2.59.2, fribidi >=  0.19.7, fontconfig >=  2.11.91, freetype2, xrender, xft >=  2.0.0, cairo >=  1.12.10
Libs: -L${libdir} -lpango-1.0
Libs.private: -lm
Cflags: -I${includedir}/pango-1.0
