prefix=/usr
libdir=${prefix}/lib
includedir=${prefix}/include

Name: Pango Cairo
Description: Cairo rendering support for Pango
Version: 1.44.6
Requires: pango, cairo >=  1.12.10
Requires.private: pangoft2, glib-2.0 >=  2.59.2, gobject-2.0 >=  2.59.2, fribidi >=  0.19.7, harfbuzz >=  2.0.0, fontconfig >=  2.11.91, freetype2, xrender, xft >=  2.0.0
Libs: -L${libdir} -lpangocairo-1.0
Libs.private: -lm
Cflags: -I${includedir}/pango-1.0
